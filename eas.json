{
  "cli": {
    "version": ">= 14.3.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "gradleCommand": ":app:assembleRelease",
      "distribution": "store",
      "android": {
        "artifact": "apk"
      },
      "channel": "production"
    }
  },
  "submit": {
    "production": {
      "track": "production"
    }
  }
}